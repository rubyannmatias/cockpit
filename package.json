{
  "name": "cockpit",
  "displayName": "Cockpit",
  "description": "",
  "version": "0.1.0",
  "publisher": "playground-lgb",
  "type": "module",
  "repository": "https://github.com/rubyannmatias/cockpit",
  "license": "MIT",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cockpit.welcome",
        "title": "%commandStr.welcome%"
      },
      {
        "command": "cockpit.configure",
        "title": "%commandStr.configure%"
      },
      {
        "command": "cockpit.execute",
        "title": "%commandStr.execute%"
      },
      {
        "command": "cockpit.ask",
        "title": "%commandStr.ask%"
      },
      {
        "command": "cockpit.history",
        "title": "%commandStr.history%"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "test-compile": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "translate": "npx @vscode/l10n-dev export -o ./l10n ./src"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vscode/test-electron": "^2.3.4",
    "esbuild": "^0.19.2",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "openai": "^4.4.0",
    "openai-node": "^1.0.5"
  },
  "l10n": "./l10n"
}
